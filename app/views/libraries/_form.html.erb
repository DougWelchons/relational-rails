<%= form_tag(url , method: method, class: 'libraries_form', local: true) do %>
  <%= label_tag :name %>
  <%= text_field_tag :name %>
  <br>
  <%= label_tag :city %>
  <%= text_field_tag :city %>
  <br>
  <%= label_tag :years_open %>
  <%= text_field_tag :years_open %>
  <br>
  <%= label_tag :open %>
  <%= check_box_tag :open %>
<%= submit_tag submit %>
<% end %>

