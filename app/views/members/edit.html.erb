<%= form_with url: "/members/#{@member.id}", method: :patch, local: true do |form| %>
  <h3><%= form.label :name %>
  <%= form.text_field :name, value: @member.name %></h3>
  <h3><%= form.label :skill_level %>
  <%= form.number_field :skill_level, value: @member.skill_level %></h3>
  <h3><%= form.label :premium_member %>
  <%= form.check_box :premium_member, {checked: @member.premium_member}%></h3>
  <%= form.submit 'Update Member' %>
<% end %>
